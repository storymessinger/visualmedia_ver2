doctype html
html
    include includes/head.pug

    block css

    body
        div.breadcrumb-row.row
            ol.breadcrumb.col-xs
                li
                    img.logo-white(src="img2/logo-white.svg" width="168px")
                li
                    img.sidebar_btn(src="img/ic_menu_white_36px.svg" fill-opacity="0" width="30px")
                    //- button.sidebar_btn(type="button")
                    //-     img(src="img/ic_menu_white_36px.svg" fill-opacity="0")

                li.breadcrumb-item
                    a(href="/") Home
                li.breadcrumb-item.active
                    a(href="/member/students") Professor

        div.main

            div.sidebar
                div.sidebar-fixed
                    ul.row.list-group
                        li.list-group-item.list-group-item-action.menu About
                        a.list-group-item.list-group-item-action.menuList.item_About(href='#') Visual Media Lab
                        a.list-group-item.list-group-item-action.menuList.item_About(href='#') Contanct
                        li.list-group-item.list-group-item-action.menu People
                        a.list-group-item.list-group-item-action.menuList.item_People(href='/member/professor') Professor
                        a.list-group-item.list-group-item-action.menuList.item_People(href='/member/students') Students
                        li.list-group-item.list-group-item-action.menu News-Events
                        a.list-group-item.list-group-item-action.menuList.item_News-Events(href='#') News
                        a.list-group-item.list-group-item-action.menuList.item_News-Events(href='#') Events
                        li.list-group-item.list-group-item-action.menu Projects
                        a.list-group-item.list-group-item-action.menuList.item_Projects(href='#') Ongoing Projects
                        a.list-group-item.list-group-item-action.menuList.item_Projects(href='/project/list') Finished Projects
                        li.list-group-item.list-group-item-action.menu Publications
                        a.list-group-item.list-group-item-action.menuList.item_Publications(href='/project/publication') 2017
                        a.list-group-item.list-group-item-action.menuList.item_Publications(href='#') 2016
                        a.list-group-item.list-group-item-action.menuList.item_Publications(href='#') Before 2015

            block content

