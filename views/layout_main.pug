                        .footer-brand-img
                            img(alt="footerLogo" src="img/logo_footer.svg")
                        ul
                            li +82 (0)42 350 5918
                            li visualmedia@kaist.ac.kr
                            li 373-1 Guseong-dong. Yueseong-Gu Daejeon, 305-701, South Korea
                    .hidden-md-down.col-lg-4.sitemap
                        .row
                            ul.col-xs Introduction
                                li
                                    a(href="#") About us
                                li
                                    a(href="#") Partners
                                li
                                    a(href="#") Admission
                            ul.col-xs Member
                                li
                                    a(href="#") Professor
                                li
                                    a(href="#") Students
                                li
                                    a(href="#") Former members
                            ul.col-xs Research
                                li
                                    a(href="#") Projects
                                li
                                    a(href="#") Research Area
                                li
                                    a(href="#") Publication
                                li
                                    a(href="#") Publication(domestic)
                                li
                                    a(href="#") Thesis
                            ul.col-xs News and Events
                                li
                                    a(href="#") Recent News
                                li
                                    a(href="#") Recent Events
                    .col-xs-12.col-sm-6.col-lg-4.footer-partner
                        .footer-partner-img
